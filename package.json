{
  "name": "infinibrains-website",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "front",
    "back"
  ],
  "scripts": {
    "postinstall": "npm install --prefix back",
    "local:back": "npm run start:nodemon --prefix back",
    "local": "concurrently \"npm run local:front\" \"npm run local:back\"",
    "local:front": "next dev",
    "prebuild:local:front": "../node_modules/.bin/rimraf .next",
    "build:local:front": "npx next build",
    "start:local:front": "npx next start -p 3000",
    "build:local:back": "npx tsc --project back/tsconfig.server.json",
    "start:local:back": "node dist/main.js",
    "start:local": "concurrently \"npm run start:local:front\" \"npm run start:local:back\"",
    "build:local": "npm run build:local:front && npm run build:local:back"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.1"
  }
}
