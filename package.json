{
  "name": "nestnext8",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/main.ts",
    "build:clean": "rimraf .next",
    "build:ui": "npx next build",
    "build:server": "npx tsc --project tsconfig.server.json && npx babel .next/production-server -d .next/production-server --extensions \".js\"",
    "build": "npm run build:clean && npm run build:ui && npm run build:server",
    "start": "cross-env NODE_ENV=production node .next/production-server/src/main.js",
    "typeorm": "typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "migration:generate": "cd src/migrations && npx typeorm-ts-node-commonjs migration:generate -d ../../ormconfig.ts",
    "migration:revert": "npm run typeorm -- migration:revert",
    "schema:drop": "npm run typeorm -- schema:drop"
  },
  "dependencies": {
    "@dataui/crud": "^5.2.3",
    "@dataui/crud-request": "^5.2.3",
    "@dataui/crud-typeorm": "^5.2.3",
    "@dataui/crud-util": "^5.2.3",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^8.4.7",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-env": "7.0.3",
    "express-ctx": "^0.1.1",
    "lodash": "^4.17.21",
    "nest-next": "10.1.0",
    "next": "12.3.4",
    "pg": "^8.10.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "0.3.16",
    "typeorm-transactional": "^0.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@nestjs/testing": "^8.4.7",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.194",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
