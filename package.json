{
  "name": "infinibrains-website",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "front",
    "back"
  ],
  "scripts": {
    "_postinstall": "npm install --prefix back",
    "_local:back": "npm run start:nodemon --prefix back",
    "_local": "concurrently \"npm run _local:front\" \"npm run _local:back\"",
    "_local:front": "cd front && next dev",
    "_prebuild:local:front": "../node_modules/.bin/rimraf .next",
    "_build:local:front": "cd front && npx next build",
    "_start:local:front": "cd front && npx next start -p 8081",
    "_build:local:back": "cd back && npx tsc --project back/tsconfig.server.json",
    "_start:local:back": "cd back && node dist/main.js",
    "_start:local": "concurrently \"npm run _start:local:front\" \"npm run _start:local:back\"",
    "_build:local": "npm run _build:local:front && npm run _build:local:back"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.1.1",
    "next": "^13.5.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/multer": "^1.4.8",
    "rimraf": "^5.0.1"
  }
}
